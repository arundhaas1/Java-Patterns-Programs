package com.dhas;

import java.util.Arrays;
import java.util.Scanner;

public class Main {
    public static void main(String []args){
       int lines=6;

       for (int i=lines;i>0;i--){
           for (int j=1;j<i;j++){
               System.out.print(j+" ");
           }
           System.out.println();
       }
    }
}

